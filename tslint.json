{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "ordered-imports": false,
        "interface-name": false,
        "eofline": {
            "severity": "warning"
        },
        "variable-name": false,
        "quotemark": {
            "severity": "warning",
            "options": ["single"]
        },
        "max-line-length":false,
        "typedef": [
            true,
            "call-signature",
            "parameter",
            "member-variable-declaration",
            "property-declaration"
        ],
        "whitespace": {
            "severity": "warning",
            "options": [
                "check-branch",
                "check-decl",
                "check-module",
                "check-operator",
                "check-preblock",
                "check-rest-spread",
                "check-separator",
                "check-type",
                "check-type-operator",
                "check-typecast"
            ]
        },
        "one-line": {
            "severity": "warning",
            "options": [
                "check-catch",
                "check-else",
                "check-finally",
                "check-open-brace",
                "check-whitespace"
            ]
        },
        "member-access": [true, "no-public"],
        "member-ordering": false,
        "no-empty": [false],
        "max-classes-per-file": [true, 3],
        "semicolon": [true, "always"]
    },
    "rulesDirectory": []
}
